@Article{AbeT90,
  Title                    = {{On some control problems in fluid mechanics.}},
  Author                   = {Abergel, F. and Temam, R.},
  Journal                  = {Theor. Comput. Fluid Dyn.},
  Year                     = {1990},
  Number                   = {6},
  Pages                    = {303-325},
  Volume                   = {1},

  Abstract                 = {{Summary: The issue of minimizing turbulence in an evolutionary Navier-Stokes flow is addressed from the point of view of optimal control. We derive theoretical results for various physical situations: distributed control, B\'enard-type problems with boundary control, and flow in a channel. For each case that we consider, our results include the formulation of the problem as an optimal control problem and proof of the existence of an optimal control (which is not expected to be unique). Finally, we describe a numerical algorithm based on the gradient method for the corresponding cost function. For readers who are not interested in the mathematical details and the mathematical justifications, a nontechnical description of our results is included.}},
  Classmath                = {{*76M25 (Other numerical methods) 76F99 (Turbulence) }},
  Comment                  = {& Ordner SKM},
  Doi                      = {10.1007/BF00271794},
  File                     = {:home/heiland/moi/papers/control/AbeT90.pdf:PDF},
  Keywords                 = {{minimizing turbulence; evolutionary Navier-Stokes flow; optimal control; B\'enard-type problems}},
  Language                 = {English}
}

